{
  "projects": {
    "default": "cvreview-d1d4b"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": ".",
      "codebase": "default",
      "runtime": "python311",
      "ignore": [
        "venv",
        ".venv",
        "env",
        ".env",
        "ENV",
        "myenv",
        "*/myenv",
        "sherlock-bot/myenv",
        "sherlockbot",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "__pycache__",
        "**/__pycache__",
        "*.pyc",
        "*.pyo",
        ".env*",
        "test*",
        "tests",
        ".pytest_cache",
        "*.gpg",
        "packages.microsoft.gpg",
        "-H",
        "-d",
        "docs",
        ".firebase",
        "*.so",
        "**/*.so",
        "lib",
        "lib64",
        "include",
        "bin",
        "share",
        ".gitignore",
        ".gcloudignore",
        "README.md",
        "*.md"
      ]
    }
  ],
  "hosting": {
    "public": "static",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}